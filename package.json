    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.0.5",
    "lucide-react": "^0.331.0",
    "next": "14.1.0",
    "openai": "^4.95.0",
    "react": "^18",
    "react-dom": "^18",
    "redis": "^4.7.0",
    "tailwind-merge": "^2.6.0",
    "thirdweb": "^5.0.0-beta-0c28573eec3111e724870f2832c1d3446a505e78-20240328000332",
    "twitter-api-v2": "^1.22.0"
  },
