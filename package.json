{
  "name": "msty-freedom-calculator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .next node_modules",
    "prepare": "husky install",
    "publish:genesis": "ts-node --project tsconfig.json src/scripts/publishGenesisPost.ts",
    "publish:market": "ts-node --project tsconfig.json src/scripts/publishBitcoinBlog.ts market",
    "publish:adoption": "ts-node --project tsconfig.json src/scripts/publishBitcoinBlog.ts adoption",
    "publish:technical": "ts-node --project tsconfig.json src/scripts/publishBitcoinBlog.ts technical",
    "publish:from-notes": "ts-node --project tsconfig.json src/scripts/generateFromNotebook.ts",
    "test:opensea": "tsx src/scripts/test-opensea.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.10",
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-progress": "^1.1.3",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.8",
    "@radix-ui/react-tooltip": "^1.2.3",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.73.3",
    "@tryghost/content-api": "^1.11.22",
    "@types/redis": "^4.0.11",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.34.8",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "ethers": "^5.7.2",
    "framer-motion": "^12.7.4",
    "lucide-react": "^0.501.0",
    "next": "^14.1.0",
    "openai": "^4.95.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.16.0",
    "recharts": "^2.15.3",
    "redis": "^4.7.0",
    "tailwind-merge": "^3.2.0",
    "twitter-api-v2": "^1.22.0",
    "viem": "^2.27.2",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/connect": "^3.4.38",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/tryghost__content-api": "^1.3.17",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "postcss": "^8.5.3",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^10.1.5",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
