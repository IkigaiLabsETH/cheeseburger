    "lint-staged": "^15.5.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
